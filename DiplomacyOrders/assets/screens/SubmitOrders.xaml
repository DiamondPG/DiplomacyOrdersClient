<UserControl x:Class="DiplomacyOrders.assets.screens.SubmitOrders"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DiplomacyOrders.assets.screens"
             mc:Ignorable="d" 
             d:DesignHeight="410" d:DesignWidth="600">
    <Grid> 
        <TextBlock Text="Submit Orders" HorizontalAlignment="Center" FontSize="24" Foreground="White"/>
        <TextBox x:Name="OrdersTXT" Background="#363636" Foreground="White" Margin="15,50,15,15" PreviewKeyDown="OrdersTXT_PreviewKeyDown"/>
        
    </Grid>
</UserControl>
